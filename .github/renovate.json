{
  "extends": [
    "config:base",
    ":dependencyDashboard",
    ":semanticPrefixChore",
    ":rebaseStalePrs",
    ":prConcurrentLimit10"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["com.github.hmcts:sscs-common"],
      "prPriority": 100
    },
    {
      "matchPackageNames": ["com.github.hmcts:sscs-common"],
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true
    }
  ]
  "labels": ["dependencies"],
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
}
